{
    "name": "ovr/phpsa",
    "description": "Static Analysis for PHP",
    "type": "util",
    "bin": [
        "bin/phpsa"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Patsura",
            "email": "talk@dmtry.me",
            "homepage": "https://github.com/ovr"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/ovr/phpsa/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ovr/phpreflection": "~0.3.1",
        "symfony/console": "~3.0.5",
        "symfony/event-dispatcher": "~3.0.5",
        "symfony/config": "~3.0.5",
        "symfony/yaml": "~3.0.5",
        "nikic/php-parser": "~2.1.0",
        "phpdocumentor/reflection-docblock": "~2.0.4",
        "phpdocumentor/type-resolver": "^0.2.0",
        "webiny/event-manager": "~1.4.1",
        "regex-guard/regex-guard": "~1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8.26",
        "squizlabs/php_codesniffer": "~2.6",
        "symfony/var-dumper": "~3.1.0"
    },
    "autoload": {
        "files": ["src/functions.php"],
        "psr-4": {
            "PHPSA\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\PHPSA\\": "tests/PHPSA"
        }
    }
}
